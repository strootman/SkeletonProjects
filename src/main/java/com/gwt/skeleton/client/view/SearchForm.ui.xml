<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	         xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	         xmlns:c="urn:import:com.sencha.gxt.widget.core.client" 
	         xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container" 
	         xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form" 
	         xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">

	<ui:with field="verticalLayoutData"
			 type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="-1" />
	</ui:with>
	
	<ui:with field="otherverticalLayoutData"
			 type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="30" />
	</ui:with>
	
	<ui:with field="margins0500"
			 type="com.sencha.gxt.core.client.util.Margins">
		<ui:attributes top="0" right="5" bottom="0" left="0" />
	</ui:with>


	<ui:with field="boxLayoutDataMargins0500"
			 type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData">
		<ui:attributes margins="{margins0500}" />
	</ui:with>
	<ui:with field="boxLayoutDataMargins0500Flex1"
			 type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData">
		<ui:attributes margins="{margins0500}" flex="1" />
	</ui:with>

	<ui:with field="padding5"
			 type="com.sencha.gxt.core.client.util.Padding">
		<ui:attributes top="5" right="5" bottom="5" left="5" />
	</ui:with>

	<container:VerticalLayoutContainer>

		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Filename has the words"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="fileName" 
									allowBlank="false"
						            emptyText="Enter values..." />
				</form:widget>
			</form:FieldLabel>
		</container:child>

		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Filename doesn't have"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="fileNameNegated"/>
				</form:widget>
			</form:FieldLabel>
		</container:child>

		
		<container:child layoutData="{verticalLayoutData}">
					<form:FieldLabel text="Created within">
						<form:widget>
							<container:HBoxLayoutContainer padding="{padding5}" hBoxLayoutAlign="TOP">
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:SimpleComboBox ui:field="createdWithinCombo" 
												   forceSelection="true" 
												   triggerAction="ALL"
												   width="75"/>
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500Flex1}">
									<g:Label />
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500Flex1}">
									<g:Label text="of"/>
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:DateField ui:field="createdWithin" 
													emptyText="Select date..." />
								</container:child>
							</container:HBoxLayoutContainer>
						</form:widget>
					</form:FieldLabel>
		</container:child>
		
		<container:child layoutData="{verticalLayoutData}">
					<form:FieldLabel text="Modified within">
						<form:widget>
							<container:HBoxLayoutContainer padding="{padding5}" hBoxLayoutAlign="TOP">
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:SimpleComboBox ui:field="modifiedWithinCombo" 
												   forceSelection="true" 
												   triggerAction="ALL" 
												   width="75"/>
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500Flex1}">
									<g:Label />
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500Flex1}">
									<g:Label text="of"/>
								</container:child>
								
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:DateField ui:field="modifiedWithin" 
													emptyText="Select date..." />
								</container:child>
							</container:HBoxLayoutContainer>
						</form:widget>
					</form:FieldLabel>
		</container:child>
		
		
		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Metadata has the words"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="metadata"
					                emptyText="Enter keyword(s) separated by commas"/>
				</form:widget>
			</form:FieldLabel>
		</container:child>
		
		
		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Metadata doesn't have"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="metadataNegated"
					               emptyText="Enter keyword(s) separated by commas"/>
				</form:widget>
			</form:FieldLabel>
		</container:child>
		

		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Created by"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="createdBy"/>
				</form:widget>
			</form:FieldLabel>
		</container:child>
		
		
		<container:child layoutData="{verticalLayoutData}">
			<form:FieldLabel text="Shared with"
							 labelAlign="TOP">
				<form:widget>
					<form:TextField ui:field="sharedWith"/>
				</form:widget>
			</form:FieldLabel>
		</container:child>
		

		<container:child layoutData="{otherverticalLayoutData}">
					<form:FieldLabel text="File size is bigger than">
						<form:widget>
							<container:HBoxLayoutContainer padding="{padding5}" hBoxLayoutAlign="TOP">
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:NumberField ui:field="fileSizeGreaterThan"/>
								</container:child>

								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:SimpleComboBox ui:field="greaterThanComboBox"
														 width="50"
														 triggerAction="ALL"
														 forceSelection="true"/>
								</container:child>
							</container:HBoxLayoutContainer>
						</form:widget>
					</form:FieldLabel>
		</container:child>

		<container:child layoutData="{otherverticalLayoutData}">
					<form:FieldLabel text="File size is smaller than">
						<form:widget>
							<container:HBoxLayoutContainer padding="{padding5}" hBoxLayoutAlign="TOP">
								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:NumberField ui:field="fileSizeLessThan"/>
								</container:child>

								<container:child layoutData="{boxLayoutDataMargins0500}">
									<form:SimpleComboBox ui:field="lessThanComboBox"
														 width="50"
														 triggerAction="ALL"
														 forceSelection="true"/>
								</container:child>
							</container:HBoxLayoutContainer>
						</form:widget>
					</form:FieldLabel>
		</container:child>
